    <!--  
            Robot Intelligence Lab
            Common Rig (Custom Humanoid) model
            
            @ Contact us : taemoon-jeong (motor_moon94@korea.ac.kr)
    -->
    <statistic extent="2" center="0 0 1"/>
    <compiler autolimits="true" inertiafromgeom="true" angle="radian"/>

    <default>
        <site size=".04" group="3"/>
        <!-- <geom condim="1" material="matgeom"/> -->
        <!-- <geom condim="1" contype="1" margin="0.001" solref=".02 1" solimp=".8 .8 .01" material="matgeom"/> -->
        <motor ctrlrange="-1. 1." ctrllimited="true"/>
        <default class="body">
            <!-- geoms -->
            <geom type="capsule" condim="1" solimp=".9 .99 .003" solref=".015 1" material="self" density="800" />
            <default class="thigh">
                <geom size=".06"/>
            </default>
            <default class="shin">
                <geom fromto="0 0 0 0 0 -.3"  size=".049"/>
            </default>
            <default class="foot">
                <geom size=".027"/>
                <default class="foot1">
                    <geom fromto="-.07 -.01 -0.05 .14 -.03 -0.05"/>
                </default>
                <default class="foot2">
                    <geom fromto="-.07 .01 -0.05 .14  .03 -0.05"/>
                </default>
            </default>

            <!-- joints -->
            <joint type="hinge" damping="1" stiffness="5" armature=".01" limited="true" solimplimit="0 .99 .01"/>
            <default class="joint_big">
                <joint damping="5" stiffness="10"/>
                <default class="hip_x">
                <joint/>
                </default>
                <default class="hip_z">
                <joint/>
                </default>
                <default class="hip_y">
                <joint axis="0 1 0"/>
                </default>
                <default class="joint_big_stiff">
                <joint stiffness="20"/>
                </default>
            </default>
            <default class="knee">
                <joint pos="0 0 .02"/>
            </default>
            <default class="ankle">
                <joint/>
                <default class="ankle_y">
                <joint pos="0 0 .08" axis="0 1 0" stiffness="6"/>
                </default>
                <default class="ankle_x">
                <joint pos="0 0 .04" stiffness="3"/>
                </default>
            </default>
            <default class="shoulder">
                <joint />
            </default>
            <default class="elbow">
                <joint stiffness="0"/>
            </default>

            <!-- tracking site -->
            <default class="tracking_site">
                <site type="sphere" size="0.027" rgba="1 0 0 1" group="3"/>
            </default>
        </default>
    </default>

